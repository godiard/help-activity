#: ../source/calculate.rst:61
# ef7fbe88061a4238aa6ae01e85306629
msgid "Functions in Calculate"
msgstr ""

#: ../source/calculate.rst:63
# 0ccd3aa842e3447a85edf128c32d5349
msgid "These are the list of functions available in Calculate Activity:"
msgstr ""

#: ../source/calculate.rst:65
# ad08a89a4fdc42cbb5ba9f7f925304b5
msgid "abs(x) - return absolute value of x, which means -x for x < 0"
msgstr ""

#: ../source/calculate.rst:67
# d83cee48e7e64b159520e35c48381e7f
msgid "acos(x) - return the arc cosine of x. This is the angle for which the cosine is x. Defined for -1 <= x < 1"
msgstr ""

#: ../source/calculate.rst:69
# b835619636fc4f2fa88647ccb58a6d93
msgid "acosh(x) - return the arc hyperbolic cosine of x. This is the value y for which the hyperbolic cosine equals x."
msgstr ""

#: ../source/calculate.rst:71
# 495ed5babfb64ef7a064c578443f8e35
msgid "and(x, y) - logical and returns True if x and y are True, else returns False"
msgstr ""

#: ../source/calculate.rst:73
# f3101f333cec4384a4c087ca50ed7fb4
msgid "add(x, y) - return x + y"
msgstr ""

#: ../source/calculate.rst:75
# cbd70266fac04c29bd6430adbfb18457
msgid "asin(x) - return the arc sine of x. This is the angle for which the sine is x. Defined for -1 <= x <= 1"
msgstr ""

#: ../source/calculate.rst:77
# 1c6c4e9b1895481e966f0ce551e5dcdd
msgid "asinh(x) - return the arc hyperbolic sine of x. This is the value y for which the hyperbolic sine equals x."
msgstr ""

#: ../source/calculate.rst:79
# d2bd277f7737482a913ca51e29a1aabf
msgid "atan(x) - return the arc tangent of x. This is the angle for which the tangent is x. Defined for all x"
msgstr ""

#: ../source/calculate.rst:81
# c6cad2c3bebd436ca68061b8db975836
msgid "atanh(x) - return the arc hyperbolic tangent of x. This is the value y for which the hyperbolic tangent equals x."
msgstr ""

#: ../source/calculate.rst:83
# cd5e2a3fa656451185521df044ea86fd
msgid "b10bin(x) - interpret a number written in base 10 as binary, e.g.: b10bin(10111) = 23."
msgstr ""

#: ../source/calculate.rst:85
# a88a619845dc4593a4ef01320bea8ec0
msgid "ceil(x) - return the smallest integer larger than x."
msgstr ""

#: ../source/calculate.rst:87
# bef89ef78e4049368d05b5d08e0e84c7
msgid "cos(x) - return the cosine of x. This is the x-coordinate on the unit circle at the angle x"
msgstr ""

#: ../source/calculate.rst:89
# 6cc13180a1484cf68e1693570cacfd5d
msgid "cosh(x) - return the hyperbolic cosine of x. Given by (exp(x) + exp(-x)) / 2"
msgstr ""

#: ../source/calculate.rst:91
# 8fadf02ee70a4f9f94765ea6d31a5986
msgid "div(x, y) - returns x/y where y!=0."
msgstr ""

#: ../source/calculate.rst:93
# 531ed0c6bbff40e29a92b46417d8aa73
msgid "gcd(a, b) - determine the greatest common denominator of a and b.For example, the biggest factor that is shared by the numbers 15 and 18 is 3."
msgstr ""

#: ../source/calculate.rst:95
# 862dd46e5c7f4e34b4f4a9ee46a4eede
msgid "exp(x) - return the natural exponent of x. Given by e^x"
msgstr ""

#: ../source/calculate.rst:97
# 21d5917cf6f2456591abadabddc97ee8
msgid "factorial(n) - return the factorial of n. Given by n * (n - 1) * (n - 2) * ... * 1"
msgstr ""

#: ../source/calculate.rst:99
# 26d919eff7ec42f9ab6acbb4eee53164
msgid "factorize(x) - determine the prime factors that together form x. For examples: 15 = 3 * 5."
msgstr ""

#: ../source/calculate.rst:101
# 23ce172e50664651a2c5913ab3c89be4
msgid "floor(x) - return the largest integer smaller than x."
msgstr ""

#: ../source/calculate.rst:103
# 8d5ce84034684f38aca82644d7fd0778
msgid "inv(x) - return the inverse of x, which is 1 / x"
msgstr ""

#: ../source/calculate.rst:105
# 57abe50819084b9184c19561b4396fc3
msgid "is_int(n) - determine whether n is an integer."
msgstr ""

#: ../source/calculate.rst:107
# 13e14c294bdc4288a9ed59eccb0d7093
msgid "is_prime(x) - Check if a number is a prime.For examples: is_prime(2)."
msgstr ""

#: ../source/calculate.rst:109
# 861b5d59fc7a404b9230951310c5eb88
msgid "ln(x) - return the natural logarithm of x. This is the value for which the exponent exp() equals x. Defined for x >= 0."
msgstr ""

#: ../source/calculate.rst:111
# c07dc5f2989f4a08a19521639ad499ae
msgid "log10(x) - return the base 10 logarithm of x. This is the value y for which 10^y equals x. Defined for x >= 0."
msgstr ""

#: ../source/calculate.rst:113
# a1aa01f0a2b241f382fa75afe124197f
msgid "mod(x, y) - return the modulus of x with respect to y. This is the remainder after dividing x by y."
msgstr ""

#: ../source/calculate.rst:115
# 961a7e2d03b04445964cd8b1c8cf9808
msgid "mul(x, y) - return x * y"
msgstr ""

#: ../source/calculate.rst:117
# cbb96fb8d95e4a95b79ed791bc9ddbf2
msgid "negate(x) - return -x"
msgstr ""

#: ../source/calculate.rst:119
# 26a068e6fd28401c99b0c82542c302aa
msgid "Or(x, y) - logical or. Returns True if x or y is True, else returns False"
msgstr ""

#: ../source/calculate.rst:121
# 282f4a7444574d5c8f27d39032ffa2db
msgid "pow(x, y) - return x to the power y (x**y)"
msgstr ""

#: ../source/calculate.rst:123
# 7dd47b1d042f43eca01a3599c4d09a2a
msgid "rand_float() - return a random floating point number between 0.0 and 1.0"
msgstr ""

#: ../source/calculate.rst:125
# 7e08bd3174e84bffbba41d96e40a47e1
msgid "rand_int([<maxval>]) - return a random integer between 0 and <maxval>.<maxval> is an optional argument and is set to 65535 by default."
msgstr ""

#: ../source/calculate.rst:127
# 230ffc07d8b54954b9612b0d7c944da8
msgid "round(x) - return the integer nearest to x."
msgstr ""

#: ../source/calculate.rst:129
# 1cade3e13db542fcbb8a5cbb6b184e1a
msgid "shift_left(x, y) - shift x by y bits to the left (multiply by 2 per bit)"
msgstr ""

#: ../source/calculate.rst:131
# 47e690b367b3471c861b894b86b061cc
msgid "shift_right(x, y) - shift x by y bits to the right (divide by 2 per bit)"
msgstr ""

#: ../source/calculate.rst:133
# 466c47098237492ebd79815ebb20fe49
msgid "sin(x) - return the sine of x. This is the y-coordinate on the unit circle at the angle x"
msgstr ""

#: ../source/calculate.rst:135
# 1b507db1de34469db1c11825322ac5fd
msgid "sinh(x) - return the hyperbolic sine of x. Given by (exp(x) - exp(-x)) / 2"
msgstr ""

#: ../source/calculate.rst:137
# a4faaab5c46149cda506de54e3725148
msgid "sinc(x) - return the sinc of x. This is given by sin(x) / x."
msgstr ""

#: ../source/calculate.rst:139
# 504729de89854a1e9ca53411230f1e82
msgid "sqrt(x) - return the square root of x. This is the value for which the square equals x. Defined for x >= 0."
msgstr ""

#: ../source/calculate.rst:141
# 0a8d026cdf154ccc80eb184b1f2d3c82
msgid "square(x) - return x * x"
msgstr ""

#: ../source/calculate.rst:143
# ac95f95dc64a45b0a63c4a00bcfc5a18
msgid "sub(x, y) - return x - y"
msgstr ""

#: ../source/calculate.rst:145
# 48f15c4120ab45e795f78752d0c45e4b
msgid "tan(x) - return the tangent of x. This is the slope of the line from the origin of the unit circle to the point on the unit circle defined by the angle x. Given by sin(x) / cos(x)"
msgstr ""

#: ../source/calculate.rst:147
# d3aa6f5b96e64dc8a6bf6d63ea1c28b0
msgid "tanh(x) - return the hyperbolic tangent of x. Given by sinh(x) / cosh(x)"
msgstr ""

#: ../source/calculate.rst:149
# 4e6305a52ec64829a22b30114c1b0748
msgid "xor(x, y) - logical xor. Returns True if either x is True (and y is False) or y is True (and x is False), else returns False"
msgstr ""

