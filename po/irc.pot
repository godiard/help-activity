msgstr ""

#: ../source/irc.rst:22
# 06ecb4eae0ff4841b092a34454d91e4a
msgid "IRC Commands"
msgstr ""

#: ../source/irc.rst:24
# cb2cd191885940598b24915ef6c961ec
msgid "The following commands can be typed into the input box, all preceded by a slash (/):"
msgstr ""

#: ../source/irc.rst:26
# 79699f6604ac4dd3b51cffbe4cc9060e
msgid "/join #channel - join a channel"
msgstr ""

#: ../source/irc.rst:28
# 3ee34b402ca646dea480de8d75d47fa1
msgid "/server irc.example.org - connect to a different network"
msgstr ""

#: ../source/irc.rst:30
# 1efb1c07ff834814b1c62dc1bd310871
msgid "/query user - open up a private chat window with another user"
msgstr ""

#: ../source/irc.rst:32
# 9f7457ebec8b4ce68ad4d78b29286523
msgid "/msg user message - privately message another user without opening a new window"
msgstr ""

#: ../source/irc.rst:34
# f76744b820fb4122977ae9d76df4d30a
msgid "/nick new_name - change your nickname away from the default"
msgstr ""

#: ../source/irc.rst:36
# 20cd9c6298914f22a06ffa65d6956c86
msgid "Many other commands are available, and you can create and administer your own channels as well: see the `IRC FAQ <http://www.irchelp.org/irchelp/altircfaq.html>`_ for more information."
msgstr ""

